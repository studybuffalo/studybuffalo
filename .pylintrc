[MASTER]
load-plugins=pylint_common, pylint_django
django-settings-module=config.settings.development
init-hook="from pylint.config import find_pylintrc; sys.path.append(os.path.join(os.path.dirname(find_pylintrc()), 'study_buffalo'))"

[FORMAT]
max-line-length=120

[MESSAGES CONTROL]
disable=invalid-name

[DESIGN]
max-parents=13

[TYPECHECK]
generated-members=REQUEST,acl_users,aq_parent,"[a-zA-Z]+_set{1,2}",save,delete
