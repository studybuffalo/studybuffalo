{
    "extends": [
        "airbnb",
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "plugins": [
        "react"
    ],
    "env": {
        "browser": true,
        "node": false
    },
    "rules": {
        "react/jsx-filename-extension": [
            1,
            {"extensions": [".js", ".jsx"]}
        ]
    }
}
